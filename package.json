{
  "name": "nestjs-service-template",
  "version": "1.0.0",
  "description": "Template for NestJs for general usage",
  "author": "dev.rjagtiani@gmail.com",
  "license": "MIT",
  "type": "commonjs",
  "homepage": "https://github.com/rjagtiani/nestjs-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjagtiani/nestjs-template.git"
  },
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "engines": {
    "node": ">=22.12.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "node --run build:clean && nest build",
    "build:clean": "rimraf dist; exit 0",
    "start:api": "node dist/apps/api",
    "start:api:dev": "nest start --config api.json --watch --debug=0.0.0.0:9229",
    "test": "npm run test:lint && jest",
    "test:unit": "jest tests/unit",
    "test:e2e": "jest tests/e2e",
    "test:lint": "eslint --max-warnings 0",
    "test:lint:fix": "eslint --max-warnings 0 --fix"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-fastify": "^11.1.0",
    "@nestjs/testing": "^11.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.5.0",
    "fastify-winston": "^1.0.9",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@nestjs/schematics": "^11.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-import-resolver-typescript": "^3.8.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "fs-extra": "^11.3.0",
    "globals": "^15.15.0",
    "jest": "^29.5.0",
    "jest-cucumber": "^4.4.0",
    "lint-staged": "^15.5.1",
    "nock": "^14.0.4",
    "npm": "^11.3.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  }
}
